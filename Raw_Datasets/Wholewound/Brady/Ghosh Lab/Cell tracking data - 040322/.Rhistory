library(knitr)
library(tidyverse)
library(car)
library(ggplot2)
library(ggsignif)
library(patchwork)
dataW <- read.csv("C:/Users/wrest/OneDrive/Documents/Fifth Year-DESKTOP-P7DLT30/Ghosh Lab/Cell tracking data - 040322/Wound_Violin_040322.csv")
dataW$WELL <- as.factor(dataW$TREATMENT)
dataA <- read.csv("C:/Users/wrest/OneDrive/Documents/Fifth Year-DESKTOP-P7DLT30/Ghosh Lab/Cell tracking data - 040322/All_Violin_040322.csv")
dataA$WELL <- as.factor(dataA$TREATMENT)
ggplot(dataW, aes(TREATMENT, TRACK_DISPLACEMENT)) + geom_violin(trim = FALSE) + theme_classic() + geom_boxplot(width = 0.1) + labs(title = "Control Track Displacement (μm)") + geom_signif(comparisons=list(c("Control","GSK"), c("Control","TSA")))
