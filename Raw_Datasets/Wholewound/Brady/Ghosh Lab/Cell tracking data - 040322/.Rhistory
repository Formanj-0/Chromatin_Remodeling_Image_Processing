ggplot(dataW, aes(TREATMENT, TRACK_MEAN_SPEED)) + geom_violin(trim = FALSE) + theme_classic() + geom_boxplot(width = 0.1) + labs(title = "Control Mean Speed (μm/s)") + geom_signif(comparisons=list(c("Control","GSK"), c("Control","TSA")))
ggplot(dataW, aes(TREATMENT, LINEARITY_OF_FORWARD_PROGRESSION)) + geom_violin(trim = FALSE) + theme_classic() + geom_boxplot(width = 0.1) + labs(title = "Control Linearity of Forward Progression") + geom_signif(comparisons=list(c("Control","GSK"), c("Control","TSA")))
ggplot(dataA, aes(TREATMENT, TRACK_DISPLACEMENT)) + geom_violin(trim = FALSE) + theme_classic() + geom_boxplot(width = 0.1) + labs(title = "Control Track Displacement (μm)") + geom_signif(comparisons=list(c("Control","GSK"), c("Control","TSA")))
ggplot(dataA, aes(TREATMENT, TOTAL_DISTANCE_TRAVELED)) + geom_violin(trim = FALSE) + theme_classic() + geom_boxplot(width = 0.1) + labs(title = "Control Track Distance (μm)") + geom_signif(comparisons=list(c("Control","GSK"), c("Control","TSA")))
ggplot(dataA, aes(TREATMENT, TRACK_MEAN_SPEED)) + geom_violin(trim = FALSE) + theme_classic() + geom_boxplot(width = 0.1) + labs(title = "Control Mean Speed (μm/s)") + geom_signif(comparisons=list(c("Control","GSK"), c("Control","TSA")))
ggplot(dataA, aes(TREATMENT, LINEARITY_OF_FORWARD_PROGRESSION)) + geom_violin(trim = FALSE) + theme_classic() + geom_boxplot(width = 0.1) + labs(title = "Control Linearity of Forward Progression") + geom_signif(comparisons=list(c("Control","GSK"), c("Control","TSA")))
